

.footer-wrap{
    position:fixed;
    bottom:0;
    left:0;
    width:100%;
    height:98rpx;
    padding-top:14rpx;
    box-sizing:border-box;
    background-color:#fff;
}

.location-item{
    padding:10rpx 30rpx;
    font-size:28rpx;
    background-color:#fff;
}

.card-wrap{
    padding:25rpx;
}

.name{
    font-size:30rpx;
    margin-bottom:30rpx;
}

.native-place{
    font-size:26rpx;
    overflow:hidden;
    white-space:nowrap;
    text-overflow:ellipsis;
}

/*scroll-view*/

.scroll-view{
    position:relative;
}

.b{
    height:100rpx;
    line-height:100rpx;
    text-align:center;
    background-color:blue;
    color:#fff;
    font-size:36rpx;
}

.c{
    position:absolute;
    width:200rpx;
    height:100%;
    line-height:100rpx;
    text-align:center;
    background-color:red;
    top:0;
    right:-200rpx;
    color:#fff;
    font-size:36rpx;
}
